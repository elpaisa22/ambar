<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.*?>

<fx:root type="javafx.scene.layout.HBox" xmlns:fx="http://javafx.com/fxml">
	<children>
	    <TextField fx:id="textField" styleClass="fld-lookup"/>
	    <Button fx:id="button" onAction="#doExecute" styleClass="btn-lookup">
		    <graphic>
		         <ImageView>
		             <image>
		                 <Image url="@../images/lookup/search.png"/>
		             </image>
		         </ImageView>
		     </graphic>
	    </Button>
	</children>
</fx:root>
